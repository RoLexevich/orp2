{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "eol-last": 0,
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
    "import/prefer-default-export": "off",
    "quotes": "off",
    "linebreak-style": 0,
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"]
  }
}
